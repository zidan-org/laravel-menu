{
  "name": "zidan-org/laravel-menu",
  "description": "Laravel Menu Builder | Drag & Drop | Bootstrap | Laravel 7 | Laravel 8 | Laravel 9 | Laravel 10 | Laravel 11",
  "keywords": [
    "Laravel",
    "Menu",
    "Bootstrap",
    "Builder",
    "Drag",
    "Drop"
  ],
  "authors": [
    {
      "name": "Mohammed Zidan",
      "email": "egytech92@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "illuminate/support": "7.* || 8.* || 9.* || 10.* || 11.*"
  },
  "autoload": {
    "psr-4": {
      "Zidan\\Menu\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Zidan\\Menu\\Providers\\MenuServiceProvider"
      ],
      "aliases": {
        "Menu": "Zidan\\Menu\\Facades\\Menu"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
